<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.input.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.libarary.INNobrary.Controller">
   <children>
      <PasswordField fx:id="passReg" layoutX="207.0" layoutY="300.0" promptText="password" />
      <PasswordField fx:id="retypepassReg" layoutX="207.0" layoutY="355.0" promptText="retype password" />
      <TextField fx:id="userReg" layoutX="207.0" layoutY="244.0" promptText="user" />
      <Text layoutX="165.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration">
         <font>
            <Font name="High Tower Text" size="51.0" />
         </font>
      </Text>
      <MenuButton fx:id="choose" layoutX="247.0" layoutY="196.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="106.0" text="Choose" textAlignment="CENTER" wrapText="true">
        <items>
          <MenuItem fx:id="Librarian" mnemonicParsing="false" onAction="#Librarian" text="Librarian" />
          <MenuItem fx:id="Reader" mnemonicParsing="false" onAction="#Reader" text="Reader" />
        </items>
      </MenuButton>
      <Button fx:id="Createacc" layoutX="231.0" layoutY="410.0" mnemonicParsing="false" onAction="#Create_an_account" text="Create an account" />
      <Label fx:id="incorrectReg" layoutX="228.0" layoutY="386.0" prefHeight="21.0" prefWidth="146.0" text="incorrect information" textFill="RED" visible="false" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="back" />
   </children>
</AnchorPane>
